@model EmpSystemApp.Models.AllModel

@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<h3> Intern</h3>

<p>
    <a asp-action="Add">Create New</a>
</p>

<div class= "row">
    <div class= "col-log-12 col-md-12">
     <table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">#EmployeeId</th>
      <th scope="col">Name</th>
      <th scope="col">Surname</th>
        <th scope="col">Age</th>
      <th scope="col">Address</th>
        <th scope="col">Department Name</th>
      <th scope="col">Position Description</th>
    </tr>
  </thead>
  <tbody>



                @{
                    if (Model.InternList !=null && Model.InternList.Count > 0)
                    {
                        foreach (var item in Model.InternList)
                        {
                            <tr>
                            
                                <td>@item.EmployeeId</td>
                                 <td>@item.Name</td>
                                  <td>@item.Surname</td>
                                   <td>@item.Age</td>
                                    <td>@item.Address</td>
                                       <td>@item.DepartmentName</td>
                                       <td>@item.Description</td>
                                     
                            </tr>
                            <tr>
                             <td>

          
           
            <td>
                <a asp-action="Update" asp-route-id="@item.EmployeeId">Update</a> |
                <a asp-action="Details" asp-route-id="@item.EmployeeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EmployeeId">Delete</a>
            </td>
        </tr>
             
                            
                        }
                    }
                }


   

  
  </tbody>
</table>
  
    

